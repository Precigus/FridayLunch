@{
    ViewBag.Title = "Friday Lunch";
}

<body>
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="main-form-container">
            <h2>Friday Lunch</h2>
            <div class="form-group">
                <input type="email" class="inputlg" placeholder="Email" id="email">
            </div>
            <div class="form-group">
                <input type="password" class="inputlg" placeholder="Old password" id="oldpassword">
            </div>
            <div class="form-group">
                <input type="password" class="inputlg" placeholder="New password" id="password">
            </div>
            <div class="form-group">
                <input type="password" class="inputlg" placeholder="Reenter new password" id="password">
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-default">Change</button>
                <li>@Html.ActionLink("ChangeCompleted", "MainWindow", "Home")</li>
            </div>
        </div>
    </div>
</body>
